{
  "schemaVersion": 1,
  "id": "minesocket",
  "version": "${version}",
  "name": "MineSocket",
  "description": "Simple minecraft mod that enables websocket communication with your server, that being a singleplayer or a multiplayer world.",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/minesocket/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.zamecki.minesocket.MineSocket"
    ],
    "client": [
      "org.zamecki.minesocket.MineSocketClient"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${compatible_minecraft_versions}",
    "fabric-permissions-api-v0": "*"
  }
}
